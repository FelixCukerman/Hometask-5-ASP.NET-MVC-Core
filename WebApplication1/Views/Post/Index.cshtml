@model List<WebApplication1.Models.Post>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Our posts</h2>

<div>
    <table border="1">
        <tr>
            <td>Title</td>
            <td>Likes</td>
            <td>Created at</td>
        </tr>
        @foreach (var post in Model)
        {
            <tr>
                <td>@post.Title</td>
                <td>@post.Likes</td>
                <td>@post.CreatedAt</td>
            </tr>
        }
    </table>
</div>